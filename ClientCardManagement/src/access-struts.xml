<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="TrnAccess" namespace="/card" extends="card-base">
		<action name="GetListTrnAccessAction" class="trnAccessAction"
			method="getList">
			<result name="success">/card/visitlog.jsp
			</result>
		</action>

		<action name="GetFromTrnAccessAction" class="trnAccessAction"
			method="getForm">
			<result name="success">/card/visitlog_form.jsp
			</result>
		</action>
		<action name="SaveTrnAccessAction" class="trnAccessAction"
			method="save">
			<result name="success" type="json"></result>
		</action>
		<action name="DeleteTrnAccessAction" class="trnAccessAction"
			method="delete">
			<result name="success" type="json"></result>
		</action>
		<action name="VisitLogSearchAction" class="trnAccessAction"
			method="search">
			<result name="success">/card/visitlog.jsp
			</result>
		</action>
	</package>
	
</struts>    
